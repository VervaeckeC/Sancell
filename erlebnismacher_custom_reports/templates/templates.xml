<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="assets_common" name="custom_reports" inherit_id="web.report_assets_common">
            <xpath expr="." position="inside">
                <link rel="stylesheet" type="text/scss" href="/erlebnismacher_custom_reports/static/src/scss/fonts.scss"/>
            </xpath>
        </template>

        <template id="_assets_primary_variables" inherit_id="website._assets_primary_variables" priority="1">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/erlebnismacher_custom_reports/static/src/scss/variables.scss"/>
            </xpath>
        </template>

        <template id="external_layout_standard_custom_reports" name="external_layout_standard_custom_reports" inherit_id="web.external_layout_standard">
            <xpath expr="//div[hasclass('header')]" position="replace">
                <div class="header">
                    <div class="row">
                        <div class="col-9"/>
                        <div class="col-3 mb4">
                            <img t-if="company.logo" t-att-src="image_data_uri(company.logo)" style="max-height: 180px;" alt="Logo"/>
                        </div>
                    </div>
                </div>
            </xpath>

            <xpath expr="//div[hasclass('footer')]" position="replace">
                <div class="footer o_standard_footer">
                    <div class="row">
                        <div class="col-2 mb4">
                            <img t-if="company.x_studio_partnerschaften" t-att-src="image_data_uri(company.x_studio_partnerschaften)" style="max-width: 1.91cm;" alt="Partnerships"/>
                        </div>
                        <div class="col-4">
                            <ul class="mb4 list-unstyled">
                                <li t-if="company.name"><span t-field="company.name"/></li>
                                <li t-if="company.street"><span t-field="company.street"/></li>
                                <t t-if="company.zip"><li><span t-field="company.zip"/> <t t-if="company.city"><span t-field="company.city"/></t></li></t>
                            </ul>
                        </div>
                        <div class="col-4 mb4">
                            <ul class="mb4 list-unstyled">
                                <li t-if="company.phone">T <span t-field="company.phone"/></li>
                                <li t-if="company.email"><span t-field="company.email"/></li>
                                <li t-if="company.website"><span t-esc="company.website.replace('http://www.', '').replace('https://www.', '')"/></li>
                                <li t-if="company.vat"><span t-field="company.vat"/></li>
                            </ul>
                        </div>
                        <div class="col-2 mb4">
                            <t t-if="company.parent_id">
                                <img t-if="company.parent_id.logo" t-att-src="image_data_uri(company.parent_id.logo)" style="max-width: 1.91cm;" alt="Logo"/>
                            </t>
                            <t t-else="">
                                <img t-if="company.logo" t-att-src="image_data_uri(company.logo)" style="max-width: 1.91cm;" alt="Logo"/>
                            </t>
                        </div>
                    </div>
                </div>
            </xpath>
        </template>

        <template id="report_saleorder_document_custom_report" inherit_id="sale.report_saleorder_document" priority="99">
            <xpath expr="//t[@t-set='address']" position="replace">
                <div style="min-height: 15mm;"/>
                <div>
                    <t t-if="doc.partner_id.parent_id"><span t-field="doc.partner_id.parent_id.name"/><br/></t>
                    <t t-if="doc.partner_id.x_studio_department"><span t-field="doc.partner_id.x_studio_department"/><br/></t>
                    <t t-if="doc.partner_id.name"><span t-field="doc.partner_id.name"/><br/></t>
                    <t t-if="doc.partner_id.street"><span t-field="doc.partner_id.street"/><br/></t>
                    <t t-if="doc.partner_id.street2"><span t-field="doc.partner_id.street2"/><br/></t>
                    <t t-if="doc.partner_id.zip"><span t-field="doc.partner_id.zip"/></t> <t t-if="doc.partner_id.city"><span t-field="doc.partner_id.city"/></t>
                </div>
                <div style="min-height: 15mm;"/>
            </xpath>

            <xpath expr="//h2" position="before">
                <p><span t-field="doc.company_id.city"/>, <span t-esc="datetime.datetime.now().strftime('%d.%m.%Y')"/></p>
            </xpath>

            <xpath expr="//div[@id='informations']" position="replace">
                <div class="row mt32 mb32" id="informations">
                    <div t-if="doc.user_id.name" class="col-3 mb4">
                        <strong>Salesperson:</strong>
                        <p class="m-0" t-field="doc.user_id"/>
                    </div>
                    <div class="col-6 mb4"/>
                    <div t-if="doc.validity_date and doc.state in ['draft', 'sent']" class="col-3 mb4">
                        <strong>Expiration Date:</strong>
                        <p class="m-0" t-field="doc.validity_date"/>
                    </div>
                </div>
            </xpath>

            <xpath expr="//p[@t-field='doc.note']" position="replace"/>

            <xpath expr="//span[@t-field='line.price_total']" position="attributes">
                <attribute name="t-options">{"widget": "float", "precision": 2}</attribute>
            </xpath>

            <xpath expr="//span[@t-field='doc.payment_term_id.note']" position="before">
                <strong>Payment Terms:</strong>
            </xpath>

            <xpath expr="//p[@t-if='doc.payment_term_id.note']" position="after">
                <p style="page-break-before:always;"> </p>
                <strong>Optional Products</strong>
                <table class="table table-sm o_main_table">
                    <thead>
                        <tr>
                            <t t-set="colspan" t-value="2"/>
                            <th class="text-left">Description</th>
                            <th class="text-right">Unit Price</th>
                        </tr>
                    </thead>
                    <tbody class="sale_tbody">

                        <t t-foreach="doc.sale_order_option_ids" t-as="line">

                            <tr t-att-class="''">
                                <td><span t-field="line.name"/></td>
                                <td class="text-right">
                                    <span t-field="line.price_unit"/>
                                </td>
                            </tr>
                        </t>
                    </tbody>
                </table>
                <p t-if="doc.note">
                    <strong>Terms and conditions</strong>
                    <table class="table table-sm o_main_table">
                        <tbody class="sale_tbody">
                            <tr t-att-class="''">
                                <td><span t-field="doc.note"/></td>
                            </tr>
                        </tbody>
                    </table>
                </p>
                <p style="page-break-before:always;"> </p>
                <strong>General information</strong>
                <table class="table table-sm o_main_table">
                    <tbody class="sale_tbody">
                        <tr t-if="doc.x_studio_programm_location" t-att-class="''">
                            <td style="width:200px">Location</td>
                            <td><span t-field="doc.x_studio_programm_location"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_start" t-att-class="''">
                            <td style="width:200px">Duration (Start - End)</td>
                            <td><span t-esc="doc.x_studio_programm_start.strftime('%d.%m.%Y %H:%M')"/> - <span t-esc="doc.x_studio_programm_end.strftime('%d.%m.%Y %H:%M')"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_lead" t-att-class="''">
                            <td style="width:200px">Leadperson</td>
                            <td><span t-field="doc.x_studio_programm_lead"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_notfallkontakt" t-att-class="''">
                            <td style="width:200px">Emergency number</td>
                            <td><span t-field="doc.x_studio_notfallkontakt"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_inkl" t-att-class="''">
                            <td style="width:200px">Included</td>
                            <td><span t-field="doc.x_studio_programm_inkl"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_exkl" t-att-class="''">
                            <td style="width:200px">Excluded</td>
                            <td><span t-field="doc.x_studio_programm_exkl"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_ausrstung" t-att-class="''">
                            <td style="width:200px">Equipment</td>
                            <td><span t-field="doc.x_studio_ausrstung"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_weather" t-att-class="''">
                            <td style="width:200px">Bad weather</td>
                            <td><span t-field="doc.x_studio_programm_weather"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_gastro" t-att-class="''">
                            <td style="width:200px">Gastronomic</td>
                            <td><span t-field="doc.x_studio_programm_gastro"/></td>
                        </tr>
                        <tr t-if="doc.x_studio_programm_more1" t-att-class="''">
                            <td style="width:200px">More</td>
                            <td><span t-field="doc.x_studio_programm_more1"/></td>
                        </tr>
                    </tbody>
                </table>

                <p style="page-break-before:always;"> </p>
                <strong>Program (Draft)</strong>
                <t t-if="doc.x_studio_title_programm1">
                    <div class="table table-sm o_main_table tso-table-image" style="page-break-inside: avoid">
