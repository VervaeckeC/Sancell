        <template id="report_invoice_document_custom_report" inherit_id="account.report_invoice_document">
            <xpath expr="//t[@t-set='address']" position="replace">
                <div style="min-height: 15mm;"/>
                <div>
                    <t t-if="o.partner_id.parent_id"><span t-field="o.partner_id.parent_id.name"/><br/></t>
                    <t t-if="o.partner_id.x_studio_department"><span t-field="o.partner_id.x_studio_department"/><br/></t>
                    <t t-if="o.partner_id.name"><span t-field="o.partner_id.name"/><br/></t>
                    <t t-if="o.partner_id.street"><span t-field="o.partner_id.street"/><br/></t>
                    <t t-if="o.partner_id.street2"><span t-field="o.partner_id.street2"/><br/></t>
                    <t t-if="o.partner_id.zip"><span t-field="o.partner_id.zip"/></t> <t t-if="o.partner_id.city"><span t-field="o.partner_id.city"/></t>
                </div>
                <div style="min-height: 15mm;"/>
            </xpath>

            <xpath expr="//h2" position="before">
                <p><span t-field="o.company_id.city"/>, <span t-field="o.date_invoice" t-options='{"widget": "date"}'/></p>
            </xpath>
        </template>


